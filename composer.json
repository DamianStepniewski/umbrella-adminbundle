{
    "name": "umbrella/adminbundle",
    "type": "symfony-bundle",
    "description": "Umbrella Admin Symfony bundle.",
    "keywords": [
        "backend",
        "admin",
        "umbrella"
    ],
    "homepage": "https://github.com/acantepie/umbrella",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Cantepie",
            "email": "adrien.cantepie@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": ">=8.1",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^3.0",
        "symfony/asset": "^7.0",
        "symfony/console": "^7.0",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/filesystem": "^7.0",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/mailer": "^7.0",
        "symfony/mime": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/options-resolver": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/yaml": "^7.0",
        "umbrella2/umbrella-corebundle": "dev-symfony7.0",
        "symfony/maker-bundle": "^1.36",
        "stof/doctrine-extensions-bundle": "^1.7"
    },
    "conflict": {
        "umbrella2/corebundle": "<6.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/phpunit-bridge": "^7.0"
    },
    "suggest": {
        "knplabs/knp-time-bundle": "Display pretty time on notification"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Umbrella\\AdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Umbrella\\AdminBundle\\Tests\\": "tests/",
            "Umbrella\\AdminBundle\\Tests\\TestApp\\": "tests/TestApplication/app/src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sniksy2/umbrella-corebundle"
        }
    ]
}
